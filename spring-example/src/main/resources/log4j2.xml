<?xml version="1.0" encoding="utf-8" ?>

<Configuration xmlns="http://logging.apache.org/log4j/2.0/config">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="file=%C line=%L %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %highlight[%-5level] - %msg%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root level="error">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</Configuration>